                                computer_options.append(9)
                                empty_positions.remove(9)
                                pygame.draw.circle(screen, (255, 255, 255), (400, 250), 19, 2)
                            elif 3 in user_positions and 3 not in empty_positions and 3 not in computer_options and 2 in empty_positions:
                                computer_options.append(2)
                                empty_positions.remove(2)                            
                                pygame.draw.circle(screen, (255, 255, 255), (350, 150), 19, 2)                                            
                            elif 7 in user_positions and 7 not in empty_positions and 7 not in computer_options and 4 in empty_positions:
                                computer_options.append(4)
                                empty_positions.remove(4)                            
                                pygame.draw.circle(screen, (255, 255, 255), (300, 200), 19, 2)                                                
                            elif 9 in user_positions and 9 not in empty_positions and 9 not in computer_options and 5 in empty_positions:
                                computer_options.append(5)
                                empty_positions.remove(5)
                                pygame.draw.circle(screen, (255, 255, 255), (350, 200), 19, 2)
                            
                            # Corner Cases
                            elif len(empty_positions) == 8:
                                computer_options.append(5)
                                empty_positions.remove(5)
                                pygame.draw.circle(screen, (255, 255, 255), (350, 200), 19, 2)
                            elif len(empty_positions) == 6 and 9 in user_positions and 5 in computer_options:
                                computer_options.append(2)
                                empty_positions.remove(2)
                                pygame.draw.circle(screen, (255, 255, 255), (350, 150), 19, 2)
                            else:
                                a = choice(empty_positions)
                                if a == 2 and a in empty_positions:
                                    computer_options.append(2)
                                    empty_positions.remove(2)
                                    pygame.draw.circle(screen, (255, 255, 255), (350, 150), 19, 2)
                                elif a == 3 and a in empty_positions:
                                    computer_options.append(3)
                                    empty_positions.remove(3)
                                    pygame.draw.circle(screen, (255, 255, 255), (400, 150), 19, 2)
                                elif a == 4 and a in empty_positions:
                                    computer_options.append(4)
                                    empty_positions.remove(4)
                                    pygame.draw.circle(screen, (255, 255, 255), (300, 200), 19, 2)
                                elif a == 5 and a in empty_positions:
                                    computer_options.append(5)
                                    empty_positions.remove(5)
                                    pygame.draw.circle(screen, (255, 255, 255), (350, 200), 19, 2)
                                elif a == 6 and a in empty_positions:                        
                                    computer_options.append(6)
                                    empty_positions.remove(6)
                                    pygame.draw.circle(screen, (255, 255, 255), (400, 200), 19, 2)                        
                                elif a == 7 and a in empty_positions:
                                    computer_options.append(7)
                                    empty_positions.remove(7)
                                    pygame.draw.circle(screen, (255, 255, 255), (300, 250), 19, 2)                        
                                elif a == 8 and a in empty_positions:
                                    computer_options.append(8)
                                    empty_positions.remove(8)
                                    pygame.draw.circle(screen, (255, 255, 255), (350, 250), 19, 2)
                                elif a == 9 and a in empty_positions:                        
                                    computer_options.append(9)
                                    empty_positions.remove(9)
                                    pygame.draw.circle(screen, (255, 255, 255), (400, 250), 19, 2)   
